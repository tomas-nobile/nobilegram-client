/nobileGram/client/src/components/User/AvatarForm/AvatarForm.js",["320","321"],"import React, { useState,useCallback } from \"react\";\nimport { Button } from \"semantic-ui-react\";\nimport { useDropzone } from \"react-dropzone\";\nimport {toast} from 'react-toastify'\nimport { useMutation } from \"@apollo/client\";\nimport { UPDATE_AVATAR, GET_USER, DELETE_AVATAR } from \"../../../gql/user\";\nimport \"./AvatarForm.scss\";\nimport AuthContext from '../../../context/AuthContext'\n\nexport default function AvatarForm(props) {\n  const { setShowModal, auth } = props;\n  const [loading,setLoading]= useState(false)\n\n  const [updateAvatar] = useMutation(UPDATE_AVATAR,{\n    \n    update(cache,{data:{updateAvatar}}){\n      const{getUser}= cache.readQuery({\n        query: GET_USER,\n        variables:{username:auth.username}\n      });\n      cache.writeQuery({\n        query:GET_USER,\n        variables:{username:auth.username},\n        data:{\n          getUser:{...getUser,avatar:updateAvatar.urlAvatar}\n        },\n      })\n    }\n  });\n\n  const [deleteAvatar]=useMutation(DELETE_AVATAR,{\n    update(cache,{data:{updateAvatar}}){\n      const{getUser}= cache.readQuery({\n        query: GET_USER,\n        variables:{username:auth.username}\n      });\n      cache.writeQuery({\n        query:GET_USER,\n        variables:{username:auth.username},\n        data:{\n          getUser:{...getUser,avatar:''}\n        },\n      })\n    }\n  });\n\n  const onDrop = useCallback(async (acceptedFile) => {\n    const file = acceptedFile[0];\n\n    try {\n      setLoading(true)  \n      const result = await updateAvatar({ variables: { file } });\n      const {data}=result\n      \n      if(!data.updateAvatar.status){\n        toast.warning('Error al actualizar el avatar');\n        setLoading(false)\n      }else{\n        setLoading(false);\n        setShowModal(false);\n      }\n    } \n    catch (error) {\n      console.log(error);\n    }\n  }, []);\n\n  const { getRootProps, getInputProps } = useDropzone({\n    accept: \"image/jpeg, image/png\",\n    noKeyboard: true,\n    multiple: false,\n    onDrop,\n  });\n\n  const onDeleteAvatar= async()=>{\n    try {\n      const result = await deleteAvatar();\n      const {data}= result;\n      if(!data.deleteAvatar){\n        toast.warning('Error al borrar el avatar')\n      }else{\n        setShowModal(false)\n      }\n    } catch (error) {\n      console.log(error);\n\n    }\n  }\n\n  return (\n    <div className=\"avatar-form\">\n      <Button {...getRootProps()} loading= {loading}>Cargar una foto</Button>\n      <Button onClick={onDeleteAvatar}>Eliminar foto actual</Button>\n      <Button onClick={() => setShowModal(false)}>Cancelar</Button>\n      <input {...getInputProps()} />\n    </div>\n  );\n}\n","/home/tomasnobile/Documentos/nobileGram/client/src/components/User/AvatarForm/index.js",[],["322","323"],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/UserNotFound/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/Profile.js",["324","325","326"],"import React, { useState } from \"react\";\nimport { Grid, Image } from \"semantic-ui-react\";\nimport { useQuery } from \"@apollo/client\";\nimport { GET_USER } from \"../../../gql/user\";\nimport userAuth from \"../../../hooks/useAuth\";\nimport UserNotFound from \"../../UserNotFound\";\nimport ModalBasic from \"../../Modal/ModalBasic\";\nimport AvatarForm from \"../AvatarForm\";\nimport HeaderProfile from \"./HeaderProfile\";\nimport SettingsForm from \"../SettingsForm\";\nimport Followers from './Followers'\nimport ImageNotFound from \"../../../assets/png/avatar.png\";\nimport \"./Profile.scss\";\n\nexport default function Profile(props) {\n  const { username, totalPublications } = props;\n  const [showModal, setShowModal] = useState(false);\n  const [titleModal, setTitleModal] = useState(\"\");\n  const [childrenModal, setChildrenModal] = useState(null);\n  const { auth } = userAuth();\n  const { data, loading, error, refetch } = useQuery(GET_USER, {\n    variables: { username },\n  });\n\n  if (loading) return null;\n\n  if (error) throw <UserNotFound />;\n\n  const { getUser } = data;\n\n  const handlerModal = (type) => {\n    switch (type) {\n      case \"avatar\":\n        setTitleModal(\"Cambiar foto de perfil\");\n        setChildrenModal(\n          <AvatarForm setShowModal={setShowModal} auth={auth} />\n        );\n        setShowModal(true);\n        break;\n\n      case \"settings\":\n        setTitleModal(\"\");\n        setChildrenModal(\n          <SettingsForm\n            setShowModal={setShowModal}\n            setTitleModal={setTitleModal}\n            setChildrenModal={setChildrenModal}\n            getUser={getUser}\n            refetch={refetch}\n          />\n        );\n        setShowModal(true);\n      default:\n        break;\n    }\n  };\n\n  return (\n    <>\n      <Grid className=\"profile\">\n        <Grid.Column width={5} className=\"profile__left\">\n          <Image\n            src={getUser.avatar ? getUser.avatar : ImageNotFound}\n            avatar\n            onClick={() => username === auth.username && handlerModal(\"avatar\")}\n          />\n        </Grid.Column>\n        <Grid.Column width={11} className=\"profile__right\">\n          <HeaderProfile\n            getUser={getUser}\n            auth={auth}\n            handlerModal={handlerModal}\n          />\n          <Followers username={username} totalPublications={totalPublications} />\n          <div className=\"other\">\n            <p className=\"name\">{getUser.name}</p>\n            {getUser.description && (\n              <p className=\"description\">{getUser.description}</p>\n            )}\n            {getUser.siteWeb && (\n              <a href={getUser.siteWeb} className=\"siteWeb\" target=\"_blank\">\n                {getUser.siteWeb}\n              </a>\n            )}\n          </div>\n        </Grid.Column>\n      </Grid>\n      <ModalBasic show={showModal} setShow={setShowModal} title={titleModal}>\n        {childrenModal}\n      </ModalBasic>\n    </>\n  );\n}\n","/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/HeaderProfile/HeaderProfile.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/PasswordForm/PasswordForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/PasswordForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/EmailForm/EmailForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/EmailForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/DescriptionForm/DescriptionForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/DescriptionForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/SettingsForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/SiteWebForm/SiteWebForm.js",[],["327","328"],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/Search/Search.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/HeaderProfile/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/Search/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/gql/follow.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/Followers/Followers.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/Followers/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/ListUsers/ListUsers.js",[],"/home/tomasnobile/Documentos/nobileGram/cli