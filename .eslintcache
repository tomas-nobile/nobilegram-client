(formData) => {\n      try {\n        const newUser= formData;\n        delete newUser.repeatPassword\n\n        const result = await register({\n          variables:{\n            input:newUser,\n          }\n        })\n        toast.success('Usuario registrado correctamente')\n      } catch (error) {\n        toast.error(error.message)\n      }\n    },\n  });\n\n  return (\n    <>\n      <h2 className=\"register-form-title\">\n        Registrate para ver fotos y videos de tus amigos\n      </h2>\n      <Form className=\"register-form\" onSubmit={formik.handleSubmit}>\n        <Form.Input\n          type=\"text\"\n          placeholder=\"Nombre y apellido\"\n          name=\"name\"\n          onChange={formik.handleChange}\n          error={formik.errors.name && true}\n        ></Form.Input>\n        <Form.Input\n          type=\"text\"\n          placeholder=\"Nombre de usuario\"\n          name=\"username\"\n          onChange={formik.handleChange}\n          error={formik.errors.username && true}\n\n        ></Form.Input>\n        <Form.Input\n          type=\"text\"\n          placeholder=\"Correo electronico\"\n          name=\"email\"\n          onChange={formik.handleChange}\n          error={formik.errors.email && true}\n        ></Form.Input>\n        <Form.Input\n          type=\"password\"\n          placeholder=\"Contraseña\"\n          name=\"password\"\n          onChange={formik.handleChange}\n          error={formik.errors.password && true}\n        ></Form.Input>\n        <Form.Input\n          type=\"password\"\n          placeholder=\"Repetir contraseña\"\n          name=\"repeatPassword\"\n          onChange={formik.handleChange}\n          error={formik.errors.repeatPassword && true}\n        ></Form.Input>\n        <Button type=\"submit\" className=\"btn-submit\">\n          Registrarse\n        </Button>\n      </Form>\n    </>\n  );\n}\n\nfunction initialValue() {\n  return {\n    name: \"\",\n    username: \"\",\n    email: \"\",\n    password: \"\",\n    repeatPassword: \"\",\n  };\n}\n","/home/tomasnobile/Documentos/nobileGram/client/src/gql/user.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Auth/RegisterForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Auth/LoginForm/LoginForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/utils/token.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/context/AuthContext.js",[],["324","325"],"/home/tomasnobile/Documentos/nobileGram/client/src/utils/constants.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/hooks/useAuth.js",["326"],"/home/tomasnobile/Documentos/nobileGram/client/src/pages/Home/Home.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/pages/Home/index.js",[],["327","328"],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Auth/LoginForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/routes/Navigation.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/pages/User.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/routes/routes.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/layouts/LayoutBasic.js",["329"],"import React, { Children } from 'react';\nimport { Container } from 'semantic-ui-react';\nimport Header from '../components/Header'\n\nexport default function LayoutBasic(props){\n    const {children}= props\n    return (\n    <>\n       <Header/>\n       <Container className=\"layout-basic\">\n        {children}\n       </Container>\n    </>\n        )\n}","/home/tomasnobile/Documentos/nobileGram/client/src/pages/Error404/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/Header.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/RightHeader/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/RightHeader/RightHeader.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/UserNotFound/UserNotFound.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/pages/Error404/Error404.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Modal/ModalBasic/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Modal/ModalBasic/ModalBasic.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/AvatarForm/AvatarForm.js",["330","331"],"import React, { useState,useCallback } from \"react\";\nimport { Button } from \"semantic-ui-react\";\nimport { useDropzone } from \"react-dropzone\";\nimport {toast} from 'react-toastify'\nimport { useMutation } from \"@apollo/client\";\nimport { UPDATE_AVATAR, GET_USER, DELETE_AVATAR } from \"../../../gql/user\";\nimport \"./AvatarForm.scss\";\nimport AuthContext from '../../../context/AuthContext'\n\nexport default function AvatarForm(props) {\n  const { setShowModal, auth } = props;\n  const [loading,setLoading]= useState(false)\n\n  const [updateAvatar] = useMutation(UPDATE_AVATAR,{\n    \n    update(cache,{data:{updateAvatar}}){\n      const{getUser}= cache.readQuery({\n        query: GET_USER,\n        variables:{username:auth.username}\n      });\n      cache.writeQuery({\n        query:GET_USER,\n        variables:{username:auth.username},\n        data:{\n          getUser:{...getUser,avatar:updateAvatar.urlAvatar}\n        },\n      })\n    }\n  });\n\n  const [deleteAvatar]=useMutation(DELETE_AVATAR,{\n    update(cache,{data:{updateAvatar}}){\n      const{getUser}= cache.readQuery({\n        query: GET_USER,\n        variables:{username:auth.username}\n      });\n      cache.writeQuery({\n        query:GET_USER,\n        variables:{username:auth.username},\n        data:{\n          getUser:{...getUser,avatar:''}\n        },\n      })\n    }\n  });\n\n  const onDrop = useCallback(async (acceptedFile) => {\n    const file = acceptedFile[0];\n\n    try {\n      setLoading(true)  \n      const result = await updateAvatar({ variables: { file } });\n      const {data}=result\n      \n      if(!data.updateAvatar.status){\n        toast.warning('Error al actualizar el avatar');\n        setLoading(false)\n      }else{\n        setLoading(false);\n        setShowModal(false);\n      }\n    } \n    catch (error) {\n      console.log(error);\n    }\n  }, []);\n\n  const { getRootProps, getInputProps } = useDropzone({\n    accept: \"image/jpeg, image/png\",\n    noKeyboard: true,\n    multiple: false,\n    onDrop,\n  });\n\n  const onDeleteAvatar= async()=>{\n    try {\n      const result = await deleteAvatar();\n      const {data}= result;\n      if(!data.deleteAvatar){\n        toast.warning('Error al borrar el avatar')\n      }else{\n        setShowModal(false)\n      }\n    } catch (error) {\n      console.log(error);\n\n    }\n  }\n\n  return (\n    <div className=\"avatar-form\">\n      <Button {...getRootProps()} loading= {loading}>Cargar una foto</Button>\n      <Button onClick={onDeleteAvatar}>Eliminar foto actual</Button>\n      <Button onClick={() => setShowModal(false)}>Cancelar</Button>\n      <input {...getInputProps()} />\n    </div>\n  );\n}\n","/home/tomasnobile/Documentos/nobileGram/client/src/components/User/AvatarForm/index.js",[],["332","333"],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Header/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/UserNotFound/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/Profile.js",["334","335","336"],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/HeaderProfile/HeaderProfile.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/SettingsForm/SettingsForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/PasswordForm/PasswordForm.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/PasswordForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/Profile/index.js",[],"/home/tomasnobile/Docu