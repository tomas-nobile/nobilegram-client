{\n          variables:{\n            file: fileUpload.file,\n          }\n        });\n        if(!result.data.publish.status){\n          toast.warning('Error en la publicacion');\n          setIsLoading(false);\n          console.log(result);\n        }else{\n          onClose()\n        }\n      } catch (error) {\n        console.log(error);\n      }\n  }\n\n  return (\n    <Modal\n      size=\"small\"\n      open={show}\n      onClose={onClose}\n      className=\"modal-upload\"\n    >\n      <div\n        {...getRootProps()}\n        className=\"dropzone\"\n        style={fileUpload && { border: 0 }}\n      >\n          {!fileUpload && (\n            <>\n        <Icon name=\"cloud upload\" />\n        <p>Arrastra tu foto que quieras publicar</p>\n        </>\n          )}\n        \n        <input {...getInputProps()} />\n      </div>\n\n      {fileUpload?.type === \"image\" && (\n        <div\n          className=\"image\"\n          style={{\n            backgroundImage: `url(\"${fileUpload.preview}\")`,\n          }}\n        />\n      )}\n\n      {fileUpload && (\n          <Button className=\"btn-upload btn-action\" onClick={onPublish}>\n              Publicar\n          </Button>\n      )}\n\n      {isLoading &&(\n        <Dimmer active className=\"publishing\">\n          <Loader />\n          <p>Publicando...</p>\n        </Dimmer>\n      )}\n    </Modal>\n  );\n}\n","/home/tomasnobile/Documentos/nobileGram/client/src/gql/publication.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/User/SiteWebForm/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Modal/ModalUpload/index.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Publications/Publications.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Publications/PreviewPublication/PreviewPublication.js",[],"/home/tomasnobile/Documentos/nobileGram/client/src/components/Modal/ModalPublication/index.js",[],"/home/tomasnobile/